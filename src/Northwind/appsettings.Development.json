{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "CacheSettings": {
    "FolderPathFromCurrentDirectory": "\\cache\\",
    "CachedExtensions": ".bmp|.png|.jpg",
    "MaxImagesCount": 8,
    "CacheTimerCleanValue": 80000
  }
}
