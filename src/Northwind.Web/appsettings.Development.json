{
  "ConnectionStrings": {
    "NorthwindDbContext": "Server=(localdb)\\mssqllocaldb;Database=Northwind;Trusted_Connection=True;"
  },
  "MaxAmountOfProducts": 0,
  "Logging": {
    "PathFormat": "logs/log-{Date}.log",
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "CacheSettings": {
    "FolderPathFromCurrentDirectory": "\\cache\\",
    "CachedExtensions": ".bmp|.png|.jpg",
    "MaxImagesCount": 8,
    "CacheTimerCleanValue": 80000
  }
}
