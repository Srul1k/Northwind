{
  "ConnectionStrings": {
    "NorthwindDbContext": "<connectionstring>",
    "NorthwindIdentityDbContext": "<connectionstring>"
  },
  "MaxAmountOfProducts": 10,
  "Logging": {
    "PathFormat": "logs/log-{Date}.log",
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "CacheSettings": {
    "FolderPathFromCurrentDirectory": "<beCarefulIfSetForLinux>",
    "CachedExtensions": ".bmp|.png|.jpg",
    "MaxImagesCount": 8,
    "CacheTimerCleanValue": 80000
  },
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "<domain>",
    "ClientId": "<client-id>",
    "TenantId": "common",
    "CallbackPath": "/signin-oidc"
  },
  "EmailService": {
    "Email": "<email>",
    "Password": "<password>"
  },
  "AdminInitializer": {
    "Email": "<email>",
    "Password": "<password>"
  }
}